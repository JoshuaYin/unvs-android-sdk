    <style name="Unvs.Activity" parent="Theme.MaterialComponents.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/unvs_primary</item>
        <item name="colorPrimaryDark">@color/unvs_primary_dark</item>
        <item name="colorPrimaryVariant">@color/unvs_primary_unable</item>
        <item name="colorOnPrimary">@android:color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/unvs_secondary</item>
        <item name="colorSecondaryVariant">@color/unvs_secondary_unable</item>
        <item name="colorOnSecondary">@android:color/white</item>
        <!-- Accent brand color. -->
        <item name="colorAccent">@color/unvs_accent</item>

        <item name="textAllCaps">false</item>
